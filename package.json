{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "snippet-template",
  "title": "Snippet Template",
  "description": "A code snippeted oriented template plugin. You can interpolate different items into your templates on the fly",
  "icon": "command-icon.png",
  "author": "lxz",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "create-snippet-templates",
      "title": "Create Snippet Template",
      "subtitle": "Create new snippet template",
      "description": "Create new snippet templates",
      "mode": "view"
    },
    {
      "name": "view-snippets-list",
      "title": "View all Snippets Template",
      "subtitle": "View all snippet templates",
      "description": "View all snippets template",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.3",
    "markdown-escape": "^2.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/markdown-escape": "^1.1.0",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
